{
    "manifest_version": 3,
    "name": "TagKeebInputEnabler",
    "version": "1.1",
    "description": "Enabled keyboard input on the aSa tag page",
    "content_scripts": [
        {
            "js": [
                "scripts/content.js"
            ],
            "matches": [
                "https://agf-wt.asa.studio/*",
                "https://agf-test-wt.asa.studio/*",
        		"https://agf-test-tt.asa.studio/*",
	        	"https://agf-tt.asa.studio/*",
                "https://agf-upgrade-wt.asa.studio/*",
        		"https://agf-upgrade-tt.asa.studio/*"
            ]
        }
    ]
}
